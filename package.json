{
  "name": "modir",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "autoformat": "lint-staged",
    "bundlesize": "bundlesize",
    "check:versions": "node test/ci/unprefixed-versions",
    "check:compile": "tsc --noEmit",
    "build": "env-cmd -f ./.env/prod.env react-app-rewired build",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.js' 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "env-cmd -f ./.env/local.env react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.5.3",
    "@amcharts/amcharts4-geodata": "4.1.6",
    "@ionic/react": "0.0.6-3",
    "classnames": "2.2.6",
    "lodash-es": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-router-last-location": "2.0.0-beta.1",
    "react-scripts": "3.0.1",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.4"
  },
  "devDependencies": {
    "@modus/gimbal": "1.1.2",
    "@modus/gimbal-plugin-axe": "1.1.2",
    "@modus/gimbal-plugin-source-map-explorer": "1.1.2",
    "@types/classnames": "2.2.8",
    "@types/d3-geo": "1.11.1",
    "@types/gapi.auth2": "0.0.50",
    "@types/lodash-es": "4.17.3",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router": "5.0.2",
    "@types/react-router-dom": "4.3.4",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.8.0",
    "@types/requestidlecallback": "0.1.1",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "env-cmd": "9.0.3",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "3.0.0",
    "lint-staged": "9.0.0",
    "node": "10.16.0",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "typescript": "3.5.2",
    "utility-types": "3.7.0",
    "webpack-gimbal-plugin": "0.1.6",
    "worker-loader": "2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run autoformat"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx},!src/models": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{json,scss,sass,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bundlesize": [
    {
      "path": "./build/precache-*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "./build/static/js/*.chunk.js",
      "maxSize": "300 kB"
    },
    {
      "path": "./build/static/js/runtime*.js",
      "maxSize": "30 kB"
    }
  ]
}
