{
  "name": "modir",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "autoformat": "lint-staged",
    "bundlesize": "bundlesize",
    "compile:once": "tsc --noEmit",
    "compile:watch": "tsc --watch --noEmit",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "lint": "tslint -p .",
    "prettier:base": "prettier 'src/**/*.{css,json,js,tsx?}'",
    "prettier": "yarn prettier:base --write",
    "prettier-check": "yarn prettier:base --list-different",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.3.5",
    "@amcharts/amcharts4-geodata": "4.1.4",
    "@ionic/react": "0.0.5",
    "classnames": "2.2.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.7.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/d3-geo": "1.11.1",
    "@types/gapi.auth2": "0.0.50",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-router": "4.4.5",
    "@types/react-router-dom": "4.3.1",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.7.0",
    "bundlesize": "0.17.1",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "node": "11.12.0",
    "prettier": "1.16.4",
    "prettier-tslint": "0.4.2",
    "react-app-rewired": "2.1.1",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "tslint-react-hooks": "2.0.0",
    "typescript": "3.4.1",
    "worker-loader": "2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "src/models/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run autoformat"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx},!src/models": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{json,scss,sass,css,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bundlesize": [{
      "path": "./build/precache-*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "./build/*.worker.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./build/static/js/*.chunk.js",
      "maxSize": "300 kB"
    },
    {
      "path": "./build/static/js/runtime*.js",
      "maxSize": "30 kB"
    }
  ]
}
