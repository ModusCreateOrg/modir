{
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "javascript.format.enable": false,
  "cSpell.words": [
    "AMPM",
    "CLOUDFLARE",
    "Fairfax",
    "Modite",
    "Modites",
    "Prefetch",
    "Reston",
    "Searchbar",
    "amcharts",
    "autoformat",
    "boundingbox",
    "browserslist",
    "bundlesize",
    "camelcase",
    "doublehit",
    "geocode",
    "geocoded",
    "geodata",
    "keyframes",
    "modir",
    "noopener",
    "noreferrer",
    "overscan",
    "precache",
    "resizable",
    "serverless",
    "serviceworker",
    "slackbot",
    "unmount",
    "unprefixed",
    "vimeo"
  ],
  "prettier.eslintIntegration": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
