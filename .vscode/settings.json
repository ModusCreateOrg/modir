{
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "javascript.format.enable": false,
  "cSpell.words": [
    "AMPM",
    "CLOUDFLARE",
    "Fairfax",
    "Modite",
    "Modites",
    "Prefetch",
    "Reston",
    "Searchbar",
    "amcharts",
    "boundingbox",
    "camelcase",
    "doublehit",
    "geocode",
    "geocoded",
    "geodata",
    "keyframes",
    "noopener",
    "noreferrer",
    "overscan",
    "resizable",
    "serverless",
    "serviceworker",
    "slackbot",
    "unmount",
    "vimeo"
  ],
  "prettier.eslintIntegration": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
